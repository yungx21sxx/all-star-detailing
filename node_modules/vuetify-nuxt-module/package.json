{
  "name": "vuetify-nuxt-module",
  "type": "module",
  "version": "0.8.0",
  "packageManager": "pnpm@8.13.1",
  "description": "Zero-Config Nuxt Module for Vuetify",
  "author": "userquin <userquin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/userquin/vuetify-nuxt-module#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/userquin/vuetify-nuxt-module.git"
  },
  "bugs": "https://github.com/userquin/vuetify-nuxt-module/issues",
  "keywords": [
    "nuxt",
    "nuxt 3",
    "vuetify",
    "vuetify 3",
    "nuxt-module"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    },
    "./custom-configuration": {
      "types": "./custom-configuration.d.ts",
      "default": "./custom-configuration.mjs"
    },
    "./configuration": {
      "types": "./configuration.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "*.cjs",
    "*.d.ts",
    "*.mjs"
  ],
  "scripts": {
    "prepack": "nuxt-module-build prepare && nuxt-module-build build && jiti scripts/postbuild.ts",
    "dev": "nuxi dev playground",
    "dev:multiple-json": "MULTIPLE_LANG_FILES=true nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev:prepare:multiple-json": "nuxt-module-build build --stub && nuxt-module-build prepare && MULTIPLE_LANG_FILES=true nuxi prepare playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:build:multiple-json": "MULTIPLE_LANG_FILES=true nuxi build playground",
    "dev:generate:multiple-json": "MULTIPLE_LANG_FILES=true nuxi generate playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare:date-io": "nuxt-module-build build --stub && nuxt-module-build prepare && pnpm nuxt prepare date-io-playground",
    "dev:date-io": "nuxi dev date-io-playground",
    "docs:dev": "pnpm -C docs run dev",
    "docs:build": "nuxt-module-build prepare && pnpm -C docs run build",
    "docs:serve": "pnpm -C docs run serve",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "publint": "publint",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.0",
    "defu": "^6.1.3",
    "destr": "^2.0.2",
    "local-pkg": "^0.5.0",
    "pathe": "^1.1.1",
    "perfect-debounce": "^1.0.0",
    "ufo": "^1.3.2",
    "unconfig": "^0.3.11",
    "vite-plugin-vuetify": "^2.0.1",
    "vuetify": "^3.4.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@antfu/ni": "^0.21.10",
    "@date-io/luxon": "^2.17.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@iconify-json/carbon": "^1.1.21",
    "@iconify-json/mdi": "^1.1.55",
    "@mdi/js": "^7.3.67",
    "@nuxt/devtools": "^1.0.6",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.9.0",
    "@nuxt/test-utils": "^3.9.0",
    "@nuxtjs/i18n": "^8.0.0",
    "@parcel/watcher": "^2.3.0",
    "@types/node": "^18",
    "@unocss/nuxt": "^0.58.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.54.0",
    "luxon": "^3.4.3",
    "nuxt": "^3.8.2",
    "publint": "^0.2.5",
    "rimraf": "^5.0.5",
    "sass": "^1.63.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.27"
  },
  "build": {
    "externals": [
      "@vuetify/loader-shared",
      "node:child_process",
      "node:fs",
      "consola",
      "destr",
      "esbuild",
      "local-pkg",
      "pathe",
      "perfect-debounce",
      "rollup",
      "upath",
      "ufo",
      "unconfig",
      "vite",
      "vite-plugin-vuetify",
      "vuetify"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "stackblitz": {
    "installDependencies": false,
    "startCommand": "node .stackblitz.js && pnpm install && nr prepack && nr dev:prepare && nr dev"
  }
}
